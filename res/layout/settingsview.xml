<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
   android:id="@+id/relativelayout1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#F0F0F0" >
    
    
 <LinearLayout
     android:id="@+id/linearLayout3"
     android:layout_width="fill_parent"
     android:layout_height="wrap_content"
     android:gravity="center"
     android:layout_marginTop="5dp"

     android:orientation="vertical" 
     android:background="@drawable/roundbutton" >
    
     <TextView
             android:layout_marginTop="5dp"
              android:layout_marginBottom="5dp"
              android:id="@+id/buttonCamera"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:onClick="buttonCamera2"
              	android:layout_weight="1"
              android:text="Settings"
              android:textSize="20dp" />
     
     
 </LinearLayout>
 
 <LinearLayout
     android:id="@+id/linearLayout4"
     android:layout_width="fill_parent"
     android:layout_height="wrap_content"
     android:gravity="center"
     android:layout_marginTop="5dp"
     android:layout_alignParentLeft="true"
     android:orientation="vertical" 
     android:layout_below="@+id/linearLayout3"
     android:background="@drawable/roundbutton" >
<LinearLayout
     android:id="@+id/linearLayout7"
     android:layout_width="fill_parent"
     android:layout_height="wrap_content"
     android:gravity="center"
     android:layout_marginTop="10dp"
 >
 <TextView
              android:id="@+id/buttonCamera"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:onClick="buttonCamera2"
              	android:layout_weight="1"
              android:text="Elinformation"
              android:textSize="20dp" />
             </LinearLayout>
  <LinearLayout
     android:id="@+id/linearLayout7"
     android:layout_width="fill_parent"
     android:layout_height="wrap_content"
     android:gravity="center"
     android:layout_marginTop="10dp"
 >

        <TextView
         android:id="@+id/buttonCamera"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
			android:layout_weight="1"
         android:onClick="buttonCamera2"
         android:text="Din fasta kostnad" />

     <EditText
         android:id="@+id/textfast"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:hint="Fyll i här"
         android:inputType="numberDecimal" />

 </LinearLayout>
     
 <LinearLayout
     android:id="@+id/linearLayout7"
     android:layout_width="fill_parent"
     android:layout_height="wrap_content"
     android:gravity="center"
     android:layout_marginTop="5dp"
 >

     <TextView
         android:id="@+id/TextView02"
         android:layout_weight="1"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:onClick="buttonCamera2"
         android:text="Det rörliga prispålägget" />

     <EditText
         android:id="@+id/textadd"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:hint="Fyll i här"
         android:inputType="numberDecimal" />

 </LinearLayout>
    
 </LinearLayout>
      


 
  <LinearLayout
     android:id="@+id/linearLayout5"
     android:layout_width="fill_parent"
     android:layout_height="wrap_content"
     android:gravity="center"
     android:layout_marginTop="5dp"
     android:layout_alignParentLeft="true"
     android:layout_below="@+id/linearLayout4"
     android:orientation="vertical" 
     android:background="@drawable/roundbutton" >
        <LinearLayout
     android:id="@+id/linearLayout7"
     android:layout_width="fill_parent"
     android:layout_height="wrap_content"
     android:gravity="center"
     android:layout_marginTop="10dp"
 >
 <TextView
              android:id="@+id/buttonCamera"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:onClick="buttonCamera2"
              	android:layout_weight="1"
              android:text="Notifikationer"
              android:textSize="20dp" />
             </LinearLayout>
    
      <LinearLayout
     android:id="@+id/linearLayout7"
     android:layout_width="fill_parent"
     android:layout_height="wrap_content"
     android:gravity="center"
     android:layout_marginTop="10dp"
 >

          <TextView
              android:id="@+id/buttonCamera"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:onClick="buttonCamera2"
              	android:layout_weight="1"
              android:text="Hög elräkning"
              android:textSize="14dp" />

     <EditText
         android:id="@+id/textHighBill"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:ems="3"
         android:inputType="numberDecimal" />

     <ToggleButton
         android:id="@+id/booleanHighBill"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
          />

 </LinearLayout>
          <LinearLayout
     android:id="@+id/linearLayout8"
     android:layout_width="fill_parent"
     android:layout_height="wrap_content"
     android:gravity="center"
     android:layout_marginTop="10dp"
     android:layout_alignParentLeft="true"
 >

     <TextView
         android:id="@+id/buttonCamera"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
			android:layout_weight="1"
         android:onClick="buttonCamera2"
         android:text="Hög användning" 
         android:textSize="14dp" />

     <EditText
         android:id="@+id/textHighUse"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:ems="3"
         android:inputType="numberDecimal" />

     <ToggleButton
         android:id="@+id/booleanHighUse"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:text="ToggleButton" />

 </LinearLayout>
 
           <LinearLayout
     android:id="@+id/linearLayout9"
     android:layout_width="fill_parent"
     android:layout_height="wrap_content"
     android:gravity="center"
     android:layout_marginTop="10dp"
     android:layout_alignParentLeft="true"
 >

     <TextView
         android:id="@+id/buttonCamera"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
			android:layout_weight="1"
         android:onClick="buttonCamera2"
         android:text="Högt elpris" 
         android:textSize="14dp" />



     <EditText
         android:id="@+id/textHighPrice"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:ems="3"
         android:inputType="numberDecimal" />

     <ToggleButton
         android:id="@+id/booleanHighPrice"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
          />

 </LinearLayout>         
          
      
  </LinearLayout>

</RelativeLayout>