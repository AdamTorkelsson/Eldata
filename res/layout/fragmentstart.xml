<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bacground"
    android:orientation="vertical" >
 

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Din Eldata"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
             android:textSize="22sp"     
             android:fontFamily="RobotoBold"  
            android:layout_gravity="center"
           />
    
  
       <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="fill_parent"
         android:layout_height="wrap_content"
      
        android:layout_marginTop="3dp"
        
        android:gravity="left"
        android:orientation="horizontal" >
    
    <LinearLayout
        android:id="@+id/linearLayout11"
        android:layout_width="0px"
         android:layout_height="wrap_content"
      android:layout_weight="3"
        android:layout_marginTop="2dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:background="@drawable/roundbutton"
        android:gravity="left"
        android:orientation="vertical" >
  	  	
  	  	<LinearLayout
  	    android:id="@+id/linearLayout111"
  	    android:layout_width="fill_parent"
  	    android:layout_height="0px"
  	    android:layout_marginTop="5dp"
  	    android:layout_weight="1"
  	    android:gravity="center_vertical"
  	    android:orientation="horizontal" >

        <TextView
            android:id="@+id/textView1"
			android:layout_width="0dip"
			android:layout_marginLeft="5dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:text="Förbrukning"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/textUse"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:hint="Error" 
            android:layout_marginRight="5dp"/>
        
                <TextView
            android:id="@+id/textUse2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:hint="Error" 
            
            android:layout_marginRight="5dp"/>
                
                <ImageButton
             android:id="@+id/imageButton1"
             android:layout_marginRight="5dp"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_gravity="top|right"
             android:background="#FCFCFC"

             android:src="@android:drawable/ic_menu_info_details" />
            
          </LinearLayout>  
          
  	<LinearLayout
  	    android:id="@+id/linearLayout111"
  	    android:layout_width="fill_parent"
  	    android:layout_height="0px"
  	    android:layout_marginTop="5dp"
  	    android:layout_weight="1"
  	    android:gravity="center_vertical"
  	    android:orientation="horizontal" >

        <TextView
            android:id="@+id/textView1"
			android:layout_width="0dip"
			android:layout_marginLeft="5dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:text="Räkning"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/textBill"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:hint="Error" 
            android:layout_marginRight="5dp"/>
        
        <TextView
            android:id="@+id/textBill2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:hint="Error" 
            android:layout_marginRight="5dp"/>
        
         <ImageButton
             android:id="@+id/imageButton2"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_gravity="top|right"
             android:background="#FCFCFC"
             android:layout_marginRight="5dp"

             android:src="@android:drawable/ic_menu_info_details" />
            
          </LinearLayout>  
          
  	
  
  	
  	  	<LinearLayout
        android:id="@+id/linearLayout112"
        android:layout_width="fill_parent"
         android:layout_height="0px"
         android:gravity="center_vertical"
      android:layout_weight="1"
        android:layout_marginTop="5dp"
        

        android:orientation="horizontal" >

             <TextView
                 android:id="@+id/textView1"
                 android:layout_width="0dip"
                 android:layout_height="wrap_content"
                 android:layout_marginLeft="5dp"
                 android:layout_gravity="center_vertical"
                 android:layout_weight="1"
                 android:gravity="left"
                 android:text="Dagar kvar"
                android:textSize="18sp" />

        <TextView
            android:id="@+id/textDayLeft"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:hint="Error" 
            android:layout_marginRight="5dp"/>
        
                 <ImageButton
             android:id="@+id/imageButton3"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_gravity="top|right"
             android:background="#FCFCFC"
             android:layout_marginRight="5dp"

             android:src="@android:drawable/ic_menu_info_details" />
          </LinearLayout>  
          
  	  	  	<LinearLayout
        android:id="@+id/linearLayout113"
        android:layout_width="fill_parent"
         android:layout_height="0px"
      android:layout_weight="1"
      android:gravity="center_vertical"
        android:layout_marginTop="5dp"
       
        android:orientation="horizontal" >
                <TextView
            android:id="@+id/textView1"
			android:layout_width="0dip"
			android:layout_marginLeft="5dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:text="Uppskattad slutsumma"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textUppskattadSlutsumma"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:hint="Error" 
            android:layout_marginRight="5dp"/>
        
                 <ImageButton
             android:id="@+id/imageButton4"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_gravity="top|right"
             android:background="#FCFCFC"
             android:layout_marginRight="5dp"

             android:src="@android:drawable/ic_menu_info_details" />
        
          </LinearLayout>  
          
  	  	  	  	<LinearLayout
        android:id="@+id/linearLayout114"
        android:layout_width="fill_parent"
        android:gravity="center_vertical"
         android:layout_height="0px"
      android:layout_weight="1"
        android:layout_marginTop="5dp"
        
 
        android:orientation="horizontal" >
               <TextView
            android:id="@+id/textView1"
			android:layout_width="0dip"
			android:layout_marginLeft="5dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:text="Över/Under snittet"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/textSnitt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:hint="Error" 
            android:layout_marginRight="5dp"/>
        
                  <ImageButton
             android:id="@+id/imageButton5"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_gravity="top|right"
             android:background="#FCFCFC"
             android:layout_marginRight="5dp"

             android:src="@android:drawable/ic_menu_info_details" />
        
        
          </LinearLayout>  
          
  	  	  	<LinearLayout
  	    android:id="@+id/linearLayout111"
  	    android:layout_width="fill_parent"
  	    android:layout_height="0px"
  	    android:layout_marginTop="5dp"
  	     android:layout_marginBottom="5dp"
  	    android:layout_weight="1"
  	    android:gravity="center_vertical"
  	    android:orientation="horizontal" >

        <TextView
            android:id="@+id/textView1"
			android:layout_width="0dip"
			android:layout_marginLeft="5dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:text="Nuvarande elpris"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/textCurrentElpris"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:hint="Error" 
            android:layout_marginRight="5dp"/>
        
                  <ImageButton
             android:id="@+id/imageButton6"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_gravity="top|right"
             android:background="#FCFCFC"
             android:layout_marginRight="5dp"

             android:src="@android:drawable/ic_menu_info_details" />
 
          </LinearLayout>
          	  	
  	  	  	  	
  	  	  	  	
  	  	  	  	
  	  	  	  	
    </LinearLayout>
    
      
    
 </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="fill_parent"
        android:layout_height="200dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="5dp"
         android:layout_marginTop="30dp"
        
        android:background="@drawable/roundbutton"
        android:gravity="left"
        android:orientation="vertical" >
    
            <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"

        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/textView1"
             android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_marginLeft="30dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="De 10 senaste dagar"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <ImageButton
            android:id="@+id/imageButton7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="3dp"
            android:background="#FCFCFC"
            android:src="@android:drawable/ic_menu_info_details" />

    </LinearLayout>
</LinearLayout>
</LinearLayout>